generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model GameState {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  current  String
  previous String
}

model Leaderboard {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  address String
  fails   BigInt
  passes  BigInt
  wins    BigInt
}
