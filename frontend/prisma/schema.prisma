generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  address String  @unique
  passes  Int     @default(0)
  fails   Int     @default(0)
  wins    Int     @default(0)

  @@map("leaderboard")
}

